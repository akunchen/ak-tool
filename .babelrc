{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": { "node": 7 }
        }],
        "es2017"
      ],
      "plugins": ["istanbul"]
    },
    "main": {
      "presets": [
        ["env", {
          "targets": { "node": 7 }
        }],
        "es2017"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "es2017"
      ]
    },
    "web": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "es2017"
      ]
    }
  },
  "plugins": ["transform-runtime"]
}
